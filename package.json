{
  "name": "@rexezuge/aws-access-bridge",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "cf-typegen": "wrangler types",
    "buildApp": "cd app/ && npm run release",
    "predeploy": "npm install && npm run cf-typegen && npm run prettier && npm run lint && npm run tsc",
    "deploy": "npm run buildApp && wrangler deploy --dry-run",
    "postdeploy": "wrangler deploy",
    "deploy:fast": "wrangler deploy",
    "prettier": "prettier --write '!app' .",
    "lint": "eslint --ext .ts,.tsx --fix --ignore-pattern app/ .",
    "tsc": "tsc --noemit",
    "migrations:apply": "yes | wrangler d1 migrations apply --remote aws-access-bridge-db"
  },
  "devDependencies": {
    "eslint": "^9.39.x",
    "eslint-config-prettier": "^10.1.x",
    "eslint-plugin-prettier": "^5.5.x",
    "globals": "^16.5.x",
    "jsonc-parser": "^3.3.x",
    "prettier": "^3.7.x",
    "typescript": "^5.9.x",
    "typescript-eslint": "^8.48.x",
    "wrangler": "^4.52.x"
  },
  "dependencies": {
    "aws4fetch": "^1.0.x",
    "axios": "^1.13.x",
    "chanfana": "^2.8.x",
    "hono": "^4.10.x",
    "jose": "^6.1.x",
    "zod": "^4.1.x"
  }
}
